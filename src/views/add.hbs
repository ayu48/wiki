<!DOCTYPE html>
    {{> head}}
    <body>
        {{> nav_bar}}
        <div class="container">
            <form name="add-page" method="post">
                <div class="form-group">
                  <label class="control-label" for="inputLarge">Title</label>
                  <input class="form-control input-lg" type="text" id="inputLarge" name="title">
                </div>
                <div class="form-group">
                    <label for="textArea" class="control-label">Body</label>
                    <textarea class="form-control" rows="15" id="textArea" name="body"></textarea>
                </div>
                <div class="bs-component">
                  <button type="submit" class="btn btn-primary">Submit</button>
                </div>
            </form>
        </div>
    </body>
</html>
